                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Desc",
                "properties": {
                    "name": {
                        "description": "name",
                        "type": "string",
                        "example": "Joe"
                    },
                    "password": {
                        "description": "password",
                        "type": "string",
                        "example": "qwerty123"
                    },
                    "email": {
                        "description": "email",
                        "type": "email",
                        "example": "Joedoe@email.com"
                    }
                },
                "type": "object"
            },
            "RegisterResponse": {
                "title": "RegisterResponse",
                "description": "Desc",
                "properties": {
                    "message": {
                        "description": "message",
